{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_core": "./src/components/core",
          "_navigations": "./src/navigations",
          "_services": "./src/services",
          "_scenes": "./src/scenes",
          "_styles": "./src/styles",
          "_theme": "./src/theme",
          "_toolbox": "./src/components/toolbox",
          "_utils": "./src/utils"
        }
      }
    ]
  ]
}
